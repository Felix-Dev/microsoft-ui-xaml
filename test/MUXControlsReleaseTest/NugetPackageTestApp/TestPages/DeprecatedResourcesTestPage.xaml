<utils:TestPage
    x:Class="NugetPackageTestApp.DeprecatedResourcesTestPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Microsoft.UI.Xaml.Controls"
    xmlns:utils="using:MUXControlsTestApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <ScrollViewer>
        <StackPanel Margin="5">
            <TextBlock Text="TabView resources"
                       Style="{StaticResource SubtitleTextBlockStyle}" Margin="0,0,0,5"/>
            <Border BorderBrush="Gray"
                    BorderThickness="1"
                    Padding="5">
                <StackPanel>
                    <TextBlock Text="TabViewItemIconForeground" Margin="0,0,0,5" />
                    <Border BorderBrush="Gray" BorderThickness="2">
                        <Rectangle Fill="{ThemeResource TabViewItemIconForeground}" MinHeight="5" MinWidth="50" />
                    </Border>
                </StackPanel>
            </Border>
        </StackPanel>
    </ScrollViewer>
</utils:TestPage>
